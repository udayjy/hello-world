pipeline {
    agent any
    environment{
        PATH = "/home/ubuntu/maven-3.8.7/bin"
    }
    stages {
        stage('git') {
            steps {
                git branch: 'main', url: 'https://github.com/udayjy/hello-world.git'
            }
        }
        stage('code build') {
            steps {
                sh "mvn clean install"
            }
          } 
       }
    }   
} 
